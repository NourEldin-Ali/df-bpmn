{
  "name": "@bpmn2-theia/workflow-theia",
  "version": "0.0.1",
  "description": "Theia extension for Imixs BPMN.glsp",
  "license": "(GPL-3.0)",
  "keywords": [
    "theia-extension"
  ],
  "author": {
    "name": "imixs.org"
  },
  "homepage": "https://www.imixs.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/imixs/imixs-bpmn.glsp.git"
  },
  "bugs": "https://github.com/imixs/imixs-bpmn.glsp/issues",
  "contributors": [
    {
      "name": "Imixs Workflow Project",
      "email": "info@imixs.org",
      "url": "https://www.imixs.org"
    }
  ],
  "files": [
    "lib",
    "src",
    "server"
  ],
  "dependencies": {
    "@imixs-bpmn/bpmn-glsp": "0.0.1", 
    "@eclipse-glsp/theia-integration": "0.9.0"
  },
  "devDependencies": {
    "mvn-artifact-download": "5.1.0",
    "rimraf": "^2.6.1",
    "ts-node": "9.0.0",
    "typescript": "^3.9.2"
  },
  "scripts": {
    "prepare": "yarn  clean && yarn  build && yarn lint",
    "clean": "rimraf lib",
    "build": "tsc",
    "lint": "eslint -c ./.eslintrc.js --ext .ts,.tsx ./src",
    "build:ci": "yarn clean && yarn build -o eslint.xml -f checkstyle",
    "watch": "tsc -w"
  },
  "publishConfig": {
    "access": "public"
  },
  "theiaExtensions": [
    {
      "frontend": "lib/browser/workflow-frontend-module",
      "backend": "lib/node/workflow-backend-module"
    }
  ]
}
