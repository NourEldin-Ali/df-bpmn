{
  "private": true,
  "name": "open-bpmn-app",
  "version": "0.4.0",
  "description": "Web App for Open BPMN",
  "license": "(GPL-3.0)",     
  "dependencies": {
    "@theia/core": "1.26.0",
    "@theia/editor": "1.26.0",
    "@theia/filesystem": "1.26.0",
    "@theia/markers": "1.26.0",
    "@theia/messages": "1.26.0",
    "@theia/monaco": "1.26.0",
    "@theia/navigator": "1.26.0",
    "@theia/preferences": "1.26.0",
    "@theia/process": "1.26.0",
    "@theia/terminal": "1.26.0",
    "@theia/workspace": "1.26.0",
    "@open-bpmn/open-bpmn-theia": "0.4.0"
  },
  "scripts": {
    "prepare": "yarn build",
    "build": "theia build --mode development",
    "start": "theia start --GLSP_PORT=5007 --root-dir=../workspace",
    "start:external": "theia start --GLSP_PORT=5007 --root-dir=../workspace --debug",
    "watch": "yarn build --mode development"
  },
  "theia": {
    "target": "browser"
  }
}
