{
  "private": true,
  "name": "bpmn-browser-app",
  "version": "0.1.0",
  "dependencies": {
    "@theia/core": "1.20.0",
    "@theia/editor": "1.20.0",
    "@theia/filesystem": "1.20.0",
    "@theia/markers": "1.20.0",
    "@theia/messages": "1.20.0",
    "@theia/monaco": "1.20.0",
    "@theia/navigator": "1.20.0",
    "@theia/preferences": "1.20.0",
    "@theia/process": "1.20.0",
    "@theia/terminal": "1.20.0",
    "@theia/workspace": "1.20.0",
    "@imixs-bpmn/bpmn-theia": "0.0.6"
  },
  "devDependencies": {
    "@theia/cli": "1.20.0"
  },
  "scripts": {
    "prepare": "yarn build",
    "build": "theia build --mode development",
    "start": "theia start --GLSP_PORT=5007 --root-dir=../workspace",
    "start:external": "theia start --GLSP_PORT=5007 --root-dir=../workspace --debug",
    "watch": "yarn build --mode development"
  },
  "theia": {
    "target": "browser"
  }
}
